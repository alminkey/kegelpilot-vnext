{
  "name": "kegelpilot-vnext",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:lan": "vite --host 0.0.0.0",
    "build": "vite build",
    "preview": "vite preview",
    "cap:copy": "vite build && npx cap copy android",
    "cap:sync": "vite build && npx cap sync",
    "cap:android": "vite build && npx cap sync android && npx cap open android",
    "studio": "npx cap open android",
    "apk:debug": "vite build && npx cap copy android && cd android && gradlew.bat assembleDebug",
    "apk:install": "cd android && gradlew.bat installDebug",
    "apk:release": "vite build && npx cap copy android && cd android && gradlew.bat assembleRelease",
    "day:end": "vite build && npx cap copy android && cd android && gradlew.bat assembleDebug",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed",
    "open:apk:debug": "start \"\" android\\app\\build\\outputs\\apk\\debug",
    "open:apk:release": "start \"\" android\\app\\build\\outputs\\apk\\release"
  },
  "dependencies": {
    "@capacitor/android": "^5.7.8",
    "@capacitor/core": "^5.7.8",
    "@stripe/stripe-js": "^7.8.0",
    "localforage": "^1.10.0",
    "svelte": "^4.2.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.7.8",
    "@playwright/test": "^1.55.0",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@types/node": "^24.3.0",
    "jsdom": "^26.1.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.0",
    "vite-plugin-mkcert": "^1.17.8",
    "vitest": "^3.2.4"
  }
}
